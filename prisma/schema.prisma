
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Schedule {
  id          String   @id @default(uuid())
  teacherName String
  room        String
  startTime   String   
  endTime     String   
  day         String   
  subject     String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@map("schedules")
}
